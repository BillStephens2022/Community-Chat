<section class="post">
  <form class="submit-profile-form">
    <!-- First Name -->
    <div class="form-outline mb-4">
      <label class="form-label" for="first-name">First Name</label>
      <input
        type="text"
        id="first-name"
        class="form-control"
        placeholder="First Name"
      />
    </div>
    <!-- Last Name -->
    <div class="form-outline mb-4">
      <label class="form-label" for="last-name">Last Name</label>
      <input
        type="text"
        id="last-name"
        class="form-control"
        placeholder="Last Name"
      />
    </div>

    <!-- Post content text area -->
    <div class="form-outline mb-4">
      <input type="date" id="date-of-birth" />
      <label for="date-of-birth">Date of Birth</label>
    </div>

    <!-- Post file area -->
    <div class="form-outline mb-4">
      <label class="form-label" for="proflie-picture">Profile Picture</label>
      <input
        type="file"
        id="proflie-picture"
        class="form-control"
        placeholder="file"
      />
    </div>
    <!-- Comment Submit button -->
    <button
      type="submit"
      class="btn btn-success btn-block mb-4 comment-submit-button"
      id="profile-submit-button"
    >Post Profile</button>

  </form>

</section>
<script src="/js/createProfile.js"></script>